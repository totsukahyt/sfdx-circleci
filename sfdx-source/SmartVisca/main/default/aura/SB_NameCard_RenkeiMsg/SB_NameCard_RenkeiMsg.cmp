<aura:component >
  <aura:attribute name="Msgs" type="List"/>
  <aura:attribute name="style" type="String"/>
  <aura:renderIf isTrue="{!not(empty(v.Msgs))}">
    <ui:message title="" severity="{!v.style}" closable="false">
      <aura:iteration var="var" items="{!v.Msgs}">
        <p>{!var}</p>
      </aura:iteration>
    </ui:message>
  </aura:renderIf>
</aura:component>